{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1",
      "includeFiles": "config.json"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.php" },
    { "src": "/", "dest": "/api/index.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
